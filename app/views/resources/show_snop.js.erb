$('#snop').html("<%=j render 'shared/snopfull' %>");
$('#prev_button').html("<%=j button_to '<', resource_show_snop_path(domain_id: @resource.domain.id, resource_id: @resource.id, snop: @prev), remote: true, disabled: @prev.nil? %>");
$('#next_button').html("<%=j button_to '>', resource_show_snop_path(domain_id: @resource.domain.id, resource_id: @resource.id, snop: @next), remote: true, disabled: @next.nil? %>");