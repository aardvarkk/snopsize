<%# show any keyword results... %>
<% if @results %>
	<% case params[:type] %>
	<% when 'url' %>
	  <%= @results.size %> URL Results found</br></br>
    <%= link_to "Be the first to snop about " + @uri.to_s, new_snop_path(uri: @uri) %>
	<% when 'user' %>
	  <%= @results.size %> User Results found</br></br>
	  <%= render :partial => 'shared/userquick', :collection => @results %>
	<% else %>
    <%= render partial: 'shared/common', locals: { snop_list: @snops, current_snop: @snops.first, data_source: search_path(q: params[:q], type: params[:type]), title: @results.size.to_s + ' Keyword Results found' } %>
	<% end %>
<% end %>